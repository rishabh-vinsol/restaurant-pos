<%= render 'shared/notice' %>
<div class="container mt-4">
<div class="border p-4">
  <h2 class="mb-4">Users</h2>
  <table class="table table-striped">
    <thead>
      <tr>
        <th>ID</th>
        <th>First Name</th>
        <th>Last Name</th>
        <th>Email</th>
        <th>Actions</th>
      </tr>
    </thead>
    <tbody>
      <%= render @users %>
    </tbody>
  </table>
  <%= link_to "New user", new_user_path %>
</div>
</div>
